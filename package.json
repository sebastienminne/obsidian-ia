  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "builtin-modules": "3.3.0",
    "esbuild": "0.17.3",
    "jest": "^30.2.0",
    "obsidian": "latest",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  }
